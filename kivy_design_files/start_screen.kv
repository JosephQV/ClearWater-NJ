<StartScreen>:
    name: "start_screen"

    FloatLayout:
    
        Image:
            source: root.app_icon_path
            size: 200, 200
            pos_hint: {"center_x": 0.50, "center_y": 0.60}

        Label:
            size: root.width/2, root.height/6
            pos_hint: {"center_x": 0.50, "center_y": 0.40}
            text: "App Title"
            